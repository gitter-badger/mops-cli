#!/usr/bin/env node

'use strict';

var program = require('commander');

program
  .version('0.0.0')
  .command('login [name]', 'Store MMS username and api key.')
  .command('topology [-g group]', 'Output MMS Topology for group')
  .command('alertsBackup [-g group]', 'Backup the alerts from a group to stdout')
  .parse(process.argv);
